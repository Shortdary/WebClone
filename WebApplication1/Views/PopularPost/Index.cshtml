@model List<Post>
@{
    ViewData["Title"] = "인기글 - 짭하하";
}

<article id="list">
    <section id="title">
        <h1>
            <div style="font-weight: 700; font-size: 21px">인기글</div>
            <div class="description">추천 많이 받으면 올라갑니다</div>
        </h1>
    </section>
    <table>
        @foreach (var p in Model)
        {
            <tr>
                <td>
                    @p.Id
                </td>
                <td>
                    @p.Subject
                </td>
            </tr>

        }
    </table>
</article>
